#include <iostream>

using namespace std;

int main()
{
double a,b,c,min,max;
cin>>a>>b>>c;
if (a>b)
{
if(a>c)
{
max=a;
if(b>c) min=c
else
min=b;
}
else
{
max=c;
min=b;
}
}
else
{
if(b>c)
{
max=b;
if(c>a) min=a;
else min=c;
}
else
{
max=c;
min=a;
}
}
cout<<"Max= "<<max<<endl<<"Min= "<<min;
return 0;
}
